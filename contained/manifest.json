{
    "manifest_version": 2,
    "name": "Contained",
    "version": "2021.10.13",
    "browser_specific_settings": {
        "gecko": {
            "id": "contained@sammacbeth.eu",
            "update_url": "https://sammacbeth.eu/addons/update.json"
        }
    },
    "background": {
        "scripts": [
            "contained.js"
        ]
    },
    "permissions": [
        "contextualIdentities",
        "cookies",
        "webNavigation",
        "storage",
        "alarms",
        "https://staticcdn.duckduckgo.com/*"
    ]
}